@model List<IoTWebApp.Models.SensorData>
@{
    ViewData["Title"] = "Home Page";
}

<h1>Temperature and Humidity Data</h1>

<table class="table">
    <thead>
        <tr>
            <th>Temperature</th>
            <th>Humidity</th>
            <th>Timestamp</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model)
        {
            <tr>
                <td>@item.Temperature</td>
                <td>@item.Humidity</td>
                <td>@item.Timestamp</td>
            </tr>
        }
    </tbody>
</table>
